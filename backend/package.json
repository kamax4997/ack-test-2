{
  "name": "apollo-backend",
  "version": "0.1.0",
  "description": "",
  "main": "./src/server.js",
  "scripts": {
    "lint": "eslint .",
    "start": "yarn && lookenv --path=./env/lookenv.dev.js --dotenv=./env/.env -- nodemon ./src/server.js --exec babel-node -r './node_modules/dotenv/config' -e js",
    "start-docker": "bash start-docker.sh",
    "add-component-part": "cd ./src/scripts && bash create-graphql-component.sh",
    "remove-component-part": "cd ./src/scripts && bash delete-graphql-component.sh",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-json.lock",
    "build": "babel -d ./build ./src -s",
    "serve-staging": "lookenv -- node ./server.js",
    "serve-production": "lookenv -- node ./server.js",
    "serve-testing": "NODE_ENV=testing babel-node ./src/server.js",
    "serve-testing-watch": "NODE_ENV=testing nodemon ./src/server.js --exec babel-node",
    "start-jest": "TERM=dumb NODE_ENV=testing jest --forceExit",
    "start-jest-watch": "NODE_ENV=testing jest --watch",
    "test": "lookenv --path=./env/lookenv.testing.js --dotenv=./env/.env -- yarn start-test serve-testing 3000 start-jest",
    "test-watch": "lookenv --path=./env/lookenv.dev.js --dotenv=./env/.env -- yarn start-test serve-testing-watch 3000 start-jest-watch",
    "requirements-check": "node ../checks/check_node_version.js",
    "preinstall": "if [ -z \"$NODE_ENV\" ]; then yarn requirements-check; fi"
  },
  "author": "",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "23.6.0",
    "babel-plugin-bulk-import": "2.0.0",
    "babel-plugin-root-import": "6.1.0",
    "dotenv": "6.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-html": "4.0.2",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-prettier": "^3.1.0",
    "graphql-tester-options": "0.0.6",
    "jest": "24.5.0",
    "jwt-decode": "2.2.0",
    "mongodb-memory-server-core": "^6.9.2",
    "nodemon": "^2.0.6",
    "prettier": "^1.18.2",
    "semver": "^7.3.4",
    "start-server-and-test": "1.7.11"
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@graphql-tools/merge": "^6.2.6",
    "@graphql-tools/schema": "^7.1.2",
    "@graphql-tools/utils": "^7.2.3",
    "apollo-datasource-mongo": "^0.1.5",
    "apollo-datasource-rest": "^0.9.6",
    "apollo-server-express": "2.14.2",
    "await-to-js": "^2.1.1",
    "base-64": "^1.0.0",
    "bcrypt": "5.0.0",
    "console-log-tree": "^1.2.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "express": "4.16.2",
    "graphql": "14.2.1",
    "graphql-constraint-directive": "^2.1.1",
    "graphql-cost-analysis": "1.0.3",
    "graphql-depth-limit": "1.1.0",
    "graphql-scalar-html": "^1.0.0",
    "graphql-scalars": "^1.1.0",
    "graphql-type-json": "^0.3.0",
    "hash.js": "^1.1.7",
    "jsonwebtoken": "8.1.1",
    "lodash.mapvalues": "^4.6.0",
    "lookenv": "1.0.1",
    "mongo-search-parameters": "^0.0.8",
    "mongodb-sanitize": "^1.0.1",
    "mongoose": "^5.9.7",
    "mongoose-delete": "^0.5.3",
    "mongoose-unique-validator": "^2.0.3",
    "string-strip-html": "^6.3.0",
    "uuid": "3.3.2",
    "yup": "^0.32.8"
  },
  "license": "MIT"
}
